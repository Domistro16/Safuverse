{
    "name": "@safuverse/safudomains-sdk",
    "version": "1.0.0",
    "description": "TypeScript SDK for SafuDomains v2 - Agent-first domain registration on Base",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm",
        "dev": "tsup src/index.ts --format cjs,esm --watch",
        "lint": "eslint src/",
        "test": "vitest run",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "ens",
        "domains",
        "base",
        "web3",
        "agent",
        "x402",
        "erc-8004"
    ],
    "author": "Safuverse",
    "license": "MIT",
    "peerDependencies": {
        "viem": "^2.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0",
        "viem": "^2.21.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Domistro16/Safuverse.git",
        "directory": "safudomains/sdk"
    }
}